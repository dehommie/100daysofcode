
    public int firstUniqChar(String s) {
        int result=-1;
    //   boolean temp= true;
        int[] counts=new int[26];
        
        for (int i=0; i<s.length();i++){
            counts[s.charAt(i) - 'a']++;
        }
        for (int f=0;f<s.length();f++){
            if(counts[s.charAt(f) - 'a']==1){
                result=f;
                return result;
            }
        }       
        
        
        
        return result;
    )
